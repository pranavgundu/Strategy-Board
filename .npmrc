# pnpm configuration

# Only allow specific trusted packages to run build scripts
onlyBuiltDependencies:
  - esbuild
  - @firebase/util
  - protobufjs

# Suppress deprecation warnings for transitive dependencies we can't control
# These come from workbox-build (peer dep of vite-plugin-pwa)
dedupe-peer-dependents=true
strict-peer-dependencies=false
